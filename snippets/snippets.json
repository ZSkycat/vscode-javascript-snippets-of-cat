{
    "import default": {
        "prefix": "imp",
        "body": "import ${2:name} from '${1:module}';$0",
        "description": "import default",
    },
    "import name": {
        "prefix": "impn",
        "body": "import { ${2:default} } from '${1:module}';$0",
        "description": "import name",
    },
    "import all": {
        "prefix": "impa",
        "body": "import * as ${2:name} from '${1:module}';$0",
        "description": "import all",
    },
    "import run": {
        "prefix": "impr",
        "body": "import '${1:module}';$0",
        "description": "import run",
    },
    "export default": {
        "prefix": "exp",
        "body": "export ${1:default} $0",
        "description": "export default",
    },
    "export from": {
        "prefix": "expf",
        "body": "export { ${2:default} } from '${1:module}';$0",
        "description": "export from",
    },
    "export all from": {
        "prefix": "expa",
        "body": "export * from '${1:module}';$0",
        "description": "export all from",
    },

    "try catch": {
        "prefix": "tryc",
        "body": [
            "try {\n\t$0",
            "} catch (error) {\n\t",
            "}",
        ],
        "description": "try catch",
    },
    "try finally": {
        "prefix": "tryf",
        "body": [
            "try {\n\t$0",
            "} finally {\n\t",
            "}",
        ],
        "description": "try finally",
    },
    "try catch finally": {
        "prefix": "trycf",
        "body": [
            "try {\n\t$0",
            "} catch (error) {\n\t",
            "} finally {\n\t",
            "}",
        ],
        "description": "try catch finally",
    },

    "getter": {
        "prefix": "getp",
        "body": [
            "get ${1:property}() {",
            "\treturn ${0:true};",
            "}",
        ],
        "description": "getter",
    },
    "setter": {
        "prefix": "setp",
        "body": "set ${1:property}(value) {\n\t$0\n}",
        "description": "setter",
    },
}
